            <Setter Property="FontSize" Value="15"/>
            <Setter Property="LineHeight" Value="22"/>
        </Style>
        
        
        <Style Selector=":is(Window)[RequestedThemeVariant=Dark] Border.message-assistant TextBlock.message-content">
