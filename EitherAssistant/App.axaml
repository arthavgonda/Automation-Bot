            <Setter Property="Padding" Value="16,10,16,10"/>
            <Setter Property="HorizontalAlignment" Value="Right"/>
            <Setter Property="MaxWidth" Value="520"/>
            <Setter Property="Margin" Value="60,4,0,4"/>
            <Setter Property="BorderThickness" Value="0"/>
