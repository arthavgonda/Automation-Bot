        <Style Selector="TextBlock.message-timestamp">
            <Setter Property="FontSize" Value="11"/>
            <Setter Property="FontWeight" Value="Medium"/>
            <Setter Property="Opacity" Value="0.5"/>
            <Setter Property="Margin" Value="0,4,0,0"/>
        </Style>
        
        
        
        
        
        <Style Selector="Border.sidebar-liquid-glass">
            <Setter Property="CornerRadius" Value="32,32,32,32"/>
            <Setter Property="BorderThickness" Value="1,1,1,1"/>
            <Setter Property="BorderBrush" Value="#607C6CFF"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="Transitions">
                <Transitions>
                    <BrushTransition Property="Background" Duration="0:0:0.6" Easing="CubicEaseInOut"/>
                    <BrushTransition Property="BorderBrush" Duration="0:0:0.6" Easing="CubicEaseInOut"/>
                </Transitions>
            </Setter>
        </Style>
        
        <Style Selector="Border.main-liquid-glass">
            <Setter Property="CornerRadius" Value="32,32,32,32"/>
            <Setter Property="BorderThickness" Value="1,1,1,1"/>
            <Setter Property="BorderBrush" Value="#607C6CFF"/>
            <Setter Property="ClipToBounds" Value="False"/>
            <Setter Property="Transitions">
                <Transitions>
                    <BrushTransition Property="Background" Duration="0:0:0.6" Easing="CubicEaseInOut"/>
                    <BrushTransition Property="BorderBrush" Duration="0:0:0.6" Easing="CubicEaseInOut"/>
                </Transitions>
            </Setter>
        </Style>
        
        
        <Style Selector="Button.sidebar-button-liquid">
            <Setter Property="Background">
                <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                    <GradientStop Color="#10FFFFFF" Offset="0"/>
                    <GradientStop Color="#05FFFFFF" Offset="1"/>
                </LinearGradientBrush>
            </Setter>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="BorderBrush" Value="#18FFFFFF"/>
            <Setter Property="Padding" Value="20,18"/>
            <Setter Property="HorizontalAlignment" Value="Stretch"/>
            <Setter Property="HorizontalContentAlignment" Value="Left"/>
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="Template">
                <ControlTemplate TargetType="Button">
                    <Border Name="PART_Border"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="22"
                            ClipToBounds="False">
                        <ContentPresenter Content="{TemplateBinding Content}"
                                        ContentTemplate="{TemplateBinding ContentTemplate}"
                                        Margin="{TemplateBinding Padding}"
                                        HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                        VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter>
            <Setter Property="Transitions">
                <Transitions>
                    <BrushTransition Property="Background" Duration="0:0:0.45" Easing="CubicEaseOut"/>
                    <BrushTransition Property="BorderBrush" Duration="0:0:0.45" Easing="CubicEaseOut"/>
                    <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.35" Easing="CubicEaseOut"/>
                </Transitions>
            </Setter>
        </Style>
        
        <Style Selector="Button.sidebar-button-liquid:pointerover">
            <Setter Property="Background">
                <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                    <GradientStop Color="#38FFFFFF" Offset="0"/>
                    <GradientStop Color="#25FFFFFF" Offset="1"/>
                </LinearGradientBrush>
            </Setter>
            <Setter Property="BorderBrush" Value="#48FFFFFF"/>
            <Setter Property="RenderTransform">
                <ScaleTransform ScaleX="1" ScaleY="1"/>
            </Setter>
            <Setter Property="Effect">
                <DropShadowEffect Color="#6C63FF" BlurRadius="35" OffsetX="0" OffsetY="0" Opacity="0.6"/>
            </Setter>
        </Style>
        
        <Style Selector="Button.sidebar-button-liquid:pressed">
            <Setter Property="RenderTransform">
                <ScaleTransform ScaleX="0.98" ScaleY="0.98"/>
            </Setter>
            <Setter Property="Effect">
                <DropShadowEffect Color="#FFFFFFFF"
                            BlurRadius="20"
                            OffsetX="0"
                            OffsetY="0"
                            Opacity="1"/>
        </Setter>
        <Setter Property="Opacity" Value="0.85"/>
        </Style>
        
        
        
        
        <Style Selector="Button.quick-select-liquid">
            <Setter Property="Background">
                <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                    <GradientStop Color="#28FFFFFF" Offset="0"/>
                    <GradientStop Color="#1AFFFFFF" Offset="1"/>
                </LinearGradientBrush>
            </Setter>
