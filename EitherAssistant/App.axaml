        <Style Selector="Button.search-button-liquid">
        <Setter Property="Background">
            <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
            <GradientStop Color="#40A49BFF" Offset="0"/>
            <GradientStop Color="#20FFFFFF" Offset="1"/>
