<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="EitherAssistant.App"
             RequestedThemeVariant="Dark">
    
    <Application.Resources>
        
        <ResourceDictionary>
            
            <SolidColorBrush x:Key="DarkPrimaryBrush" Color="#6C63FF"/>
            <SolidColorBrush x:Key="DarkBackgroundBrush" Color="#050508"/>
            <SolidColorBrush x:Key="DarkSurfaceBrush" Color="#1A1A2E"/>
            <SolidColorBrush x:Key="DarkTextBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="DarkTextSecondaryBrush" Color="#B8B8B8"/>
            <SolidColorBrush x:Key="DarkUserMessageBrush" Color="#6C63FF"/>
            <SolidColorBrush x:Key="DarkAssistantMessageBrush" Color="#2A2A3E"/>
            
            
            <SolidColorBrush x:Key="LightPrimaryBrush" Color="#007AFF"/>
            <SolidColorBrush x:Key="LightBackgroundBrush" Color="#F2F2F7"/>
            <SolidColorBrush x:Key="LightSurfaceBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="LightTextBrush" Color="#000000"/>
            <SolidColorBrush x:Key="LightTextSecondaryBrush" Color="#6C6C70"/>
            <SolidColorBrush x:Key="LightUserMessageBrush" Color="#007AFF"/>
            <SolidColorBrush x:Key="LightAssistantMessageBrush" Color="#E9E9EB"/>
            
            
            <SolidColorBrush x:Key="AmoledPrimaryBrush" Color="#00E5FF"/>
            <SolidColorBrush x:Key="AmoledBackgroundBrush" Color="#000000"/>
            <SolidColorBrush x:Key="AmoledSurfaceBrush" Color="#0A0A0A"/>
            <SolidColorBrush x:Key="AmoledTextBrush" Color="#FFFFFF"/>
            <SolidColorBrush x:Key="AmoledTextSecondaryBrush" Color="#AAAAAA"/>
            <SolidColorBrush x:Key="AmoledUserMessageBrush" Color="#00E5FF"/>
            <SolidColorBrush x:Key="AmoledAssistantMessageBrush" Color="#1A1A1A"/>
        </ResourceDictionary>
    </Application.Resources>
    
    <Application.Styles>
        <FluentTheme />
        
        
        
        
        <Style Selector=":is(Window)[RequestedThemeVariant=Dark]">
            <Style Selector="^ Border.sidebar-liquid-glass">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#35FFFFFF" Offset="0"/>
                        <GradientStop Color="#18FFFFFF" Offset="0.25"/>
                        <GradientStop Color="#22FFFFFF" Offset="0.5"/>
                        <GradientStop Color="#15FFFFFF" Offset="0.75"/>
                        <GradientStop Color="#28FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="BorderBrush">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="0%,100%">
                        <GradientStop Color="#70FFFFFF" Offset="0"/>
                        <GradientStop Color="#25FFFFFF" Offset="0.5"/>
                        <GradientStop Color="#60FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
            </Style>
            
            <Style Selector="^ Border.main-liquid-glass">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#28FFFFFF" Offset="0"/>
                        <GradientStop Color="#12FFFFFF" Offset="0.25"/>
                        <GradientStop Color="#1AFFFFFF" Offset="0.5"/>
                        <GradientStop Color="#10FFFFFF" Offset="0.75"/>
                        <GradientStop Color="#25FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
            </Style>
            
            <Style Selector="^ TextBlock.theme-text">
                <Setter Property="Foreground" Value="{DynamicResource DarkTextBrush}"/>
            </Style>
        </Style>
        
        
        
        
        <Style Selector=":is(Window)[RequestedThemeVariant=Light]">
            <Style Selector="^ Border.sidebar-liquid-glass">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#F5FFFFFF" Offset="0"/>
                        <GradientStop Color="#E8FFFFFF" Offset="0.5"/>
                        <GradientStop Color="#F0FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="BorderBrush">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="0%,100%">
                        <GradientStop Color="#E0000000" Offset="0"/>
                        <GradientStop Color="#20000000" Offset="0.5"/>
                        <GradientStop Color="#D0000000" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="Effect">
                    <DropShadowEffect Color="#000000" BlurRadius="60" OffsetX="-8" OffsetY="0" Opacity="0.15"/>
