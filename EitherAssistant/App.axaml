            </Style>
            
            <Style Selector="^ Border.main-liquid-glass">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#FAFFFFFF" Offset="0"/>
                        <GradientStop Color="#F0FFFFFF" Offset="0.5"/>
                        <GradientStop Color="#F5FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="Effect">
                    <DropShadowEffect Color="#000000" BlurRadius="60" OffsetX="8" OffsetY="0" Opacity="0.15"/>
                </Setter>
            </Style>
            
            <Style Selector="^ TextBlock.theme-text">
                <Setter Property="Foreground" Value="{DynamicResource LightTextBrush}"/>
            </Style>
            
            <Style Selector="^ Button.sidebar-button-liquid">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#15000000" Offset="0"/>
                        <GradientStop Color="#08000000" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="BorderBrush" Value="#25000000"/>
            </Style>
            
            <Style Selector="^ Button.sidebar-button-liquid:pointerover">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#30000000" Offset="0"/>
                        <GradientStop Color="#20000000" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="Effect">
                    <DropShadowEffect Color="#007AFF" BlurRadius="35" OffsetX="0" OffsetY="0" Opacity="0.5"/>
                </Setter>
            </Style>
        </Style>
        
        
        
        
        <Style Selector=":is(Window).amoled-theme">
            <Style Selector="^ Border.sidebar-liquid-glass">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#20FFFFFF" Offset="0"/>
                        <GradientStop Color="#10FFFFFF" Offset="0.5"/>
                        <GradientStop Color="#18FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
                <Setter Property="BorderBrush" Value="#40FFFFFF"/>
            </Style>
            
            <Style Selector="^ Border.main-liquid-glass">
                <Setter Property="Background">
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#15FFFFFF" Offset="0"/>
                        <GradientStop Color="#08FFFFFF" Offset="0.5"/>
                        <GradientStop Color="#12FFFFFF" Offset="1"/>
                    </LinearGradientBrush>
                </Setter>
            </Style>
            
            <Style Selector="^ TextBlock.theme-text">
                <Setter Property="Foreground" Value="{DynamicResource AmoledTextBrush}"/>
            </Style>
        </Style>
        
        
        
        
        
        
        <Style Selector=":is(Window)[RequestedThemeVariant=Dark] Border.message-user">
